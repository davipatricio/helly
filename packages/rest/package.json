{
    "name": "@hellyjs/rest",
    "version": "0.0.1",
    "description": "Minimal REST Client to make HTTP requests to Discord",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "typings": "./dist/index.d.ts",
    "exports": {
        "import": "./dist/index.mjs",
        "require": "./dist/index.js",
        "types": "./dist/index.d.ts"
    },
    "scripts": {
        "build": "tsup",
        "types:check": "tsc --noEmit"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/denkylabs/helly.git"
    },
    "bugs": {
        "url": "https://github.com/denkylabs/helly/issues"
    },
    "files": [
        "dist"
    ],
    "author": "Davi Patricio <davipatricio@pm.me>",
    "license": "Apache-2.0",
    "keywords": [
        "discord",
        "api",
        "rest",
        "discordapp",
        "helly"
    ],
    "homepage": "https://helly.denkylabs.com",
    "typedoc": {
        "entryPoint": "./src/index.ts",
        "readmeFile": "./README.md",
        "displayName": "@hellyjs/rest"
    },
    "engines": {
        "node": ">=16.9.0"
    },
    "dependencies": {
        "@sapphire/ratelimits": "2.4.5",
        "discord-api-types": "0.37.9",
        "undici": "5.11.0"
    }
}
