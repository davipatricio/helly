{
  "module": "./build/cjs/index.js",
  "main": "./build/cjs/index.js",
  "types": "./build/types/index.d.ts",
  "name": "helly",
  "version": "2.0.0",
  "private": false,
  "description": "A simple, typed, performance-focused Discord API Wrapper.",
  "type": "commonjs",
  "keywords": [
    "discord",
    "api",
    "bot",
    "client",
    "node",
    "wrapper",
    "discordapp",
    "helly"
  ],
  "files": [
    "build",
    "LICENSE",
    "package-lock.json",
    "package.json",
    "README.md"
  ],
  "exports": {
    "import": "./build/cjs/index.js",
    "require": "./build/cjs/index.js",
    "default": "./build/cjs/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/davipatricio/helly.git"
  },
  "bugs": {
    "url": "https://github.com/davipatricio/helly/issues"
  },
  "scripts": {
    "docs": "npx typedoc --options typedoc.json",
    "build": "npx tsc --project tsconfig.json",
    "test": "node tests/index",
    "lint": "npx eslint src",
    "lint:fix": "npm run lint:prettier && npx eslint src --fix",
    "lint:prettier": "npx prettier -w src",
    "types:check": " npx tsc --noEmit --incremental false"
  },
  "author": "Davi Patricio <davipatricio@pm.me>",
  "devDependencies": {
    "@types/node": "16.11.26",
    "@types/ws": "8.5.3",
    "@typescript-eslint/eslint-plugin": "5.17.0",
    "@typescript-eslint/parser": "5.17.0",
    "eslint": "8.12.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-prettier": "4.0.0",
    "prettier": "2.6.1",
    "typedoc": "0.22.13",
    "typedoc-plugin-extras": "2.2.3",
    "typescript": "4.7.0-dev.20220401"
  },
  "dependencies": {
    "@discordjs/collection": "^0.5.0",
    "@discordjs/rest": "^0.4.0-dev.1648123724.b520c3d",
    "@sapphire/ratelimits": "^2.4.3",
    "axios": "^0.26.0",
    "discord-api-types": "^0.30.0",
    "tslib": "^2.3.1",
    "ws": "^8.5.0"
  }
}
