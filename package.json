{
  "name": "helly",
  "version": "2.0.0",
  "description": "A simple, typed, performance-focused Discord API Wrapper.",
  "type": "commonjs",
  "keywords": [
    "discord",
    "api",
    "bot",
    "client",
    "node",
    "wrapper",
    "discordapp",
    "helly"
  ],
  "files": [
    "build",
    "LICENSE",
    "package-lock.json",
    "package.json",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/denkylabs/helly.git"
  },
  "bugs": {
    "url": "https://github.com/denkylabs/helly/issues"
  },
  "scripts": {
    "docs": "yarn typedoc --options typedoc.json",
    "build": "yarn tsc --project tsconfig.json",
    "lint:fix": "yarn lint:prettier && yarn eslint src --fix",
    "lint:prettier": "yarn prettier -w src",
    "types:check": "yarn tsc --noEmit --incremental false"
  },
  "author": "Davi Patricio <davipatricio@pm.me>",
  "devDependencies": {
    "@types/node": "17.0.23",
    "@types/ws": "8.5.3",
    "@typescript-eslint/eslint-plugin": "5.27.0",
    "@typescript-eslint/parser": "5.27.0",
    "eslint": "8.16.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.0.0",
    "prettier": "2.6.2",
    "typedoc": "0.22.16",
    "typedoc-plugin-extras": "2.2.3",
    "typescript": "4.7.0-dev.20220506"
  },
  "dependencies": {
    "discord-api-types": "0.33.0",
    "tslib": "2.4.0"
  },
  "packageManager": "yarn@4.0.0-rc.15"
}
