{
  "module": "./build/cjs/index.js",
  "main": "./build/cjs/index.js",
  "types": "./build/types/index.d.ts",
  "name": "helly",
  "version": "2.0.0",
  "private": false,
  "description": "A simple, typed, performance-focused Discord API Wrapper.",
  "type": "commonjs",
  "keywords": [
    "discord",
    "api",
    "bot",
    "client",
    "node",
    "wrapper",
    "discordapp",
    "helly"
  ],
  "files": [
    "./build",
    "LICENSE",
    "package-lock.json",
    "package.json"
  ],
  "exports": {
    "import": "./build/cjs/index.js",
    "require": "./build/cjs/index.js",
    "default": "./build/cjs/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/davipatricio/helly.git"
  },
  "bugs": {
    "url": "https://github.com/davipatricio/helly/issues"
  },
  "scripts": {
    "docs": "npx typedoc --options typedoc.json",
    "build": "npx tsc --project tsconfig.json",
    "lint": "npx eslint src",
    "lint:fix": "npm run lint:prettier && npx eslint src --fix",
    "lint:prettier": "npx prettier -w src",
    "types:check": " npx tsc --noEmit --incremental false"
  },
  "author": "Davi Patricio <davipatricio@pm.me>",
  "devDependencies": {
    "@types/node": "16.11.24",
    "@types/ws": "8.2.2",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "eslint": "^8.10.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.5.1",
    "typedoc": "^0.22.12",
    "typedoc-plugin-extras": "^2.2.3",
    "typescript": "^4.7.0-dev.20220227"
  },
  "dependencies": {
    "@discordjs/collection": "^0.5.0",
    "axios": "^0.25.0",
    "discord-api-types": "^0.27.3",
    "tslib": "^2.3.1",
    "ws": "^8.5.0"
  }
}
