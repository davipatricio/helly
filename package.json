{
  "name": "helly",
  "version": "1.1.0",
  "private": false,
  "description": "A simple, typed, performance-focused Discord API Wrapper.",
  "main": "./build/index.js",
  "type": "commonjs",
  "keywords": [
    "discord",
    "api",
    "bot",
    "client",
    "node",
    "wrapper",
    "discordapp",
    "helly"
  ],
  "files": [
    "build",
    "LICENSE",
    "package-lock.json",
    "package.json"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/davipatricio/helly.git"
  },
  "bugs": {
    "url": "https://github.com/davipatricio/helly/issues"
  },
  "scripts": {
    "docs": "jsdoc src -R README.md -c jsdoc.json",
    "build": "npx sucrase ./src -d ./build --esModuleInterop --transforms typescript,imports",
    "build:prod": "npm run build && npm run minify && npm run types:declarations",
    "minify": "npx uglifyjs-folder ./build --compress --mangle -eo build -x .js",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "types:check": "tsc --noEmit --incremental false",
    "types:declarations": "tsc --emitDeclarationOnly"
  },
  "author": "Davi Patricio <davipatricio@pm.me>",
  "devDependencies": {
    "@babel/core": "7.17.2",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/preset-env": "7.16.11",
    "@babel/preset-typescript": "7.16.7",
    "@types/node": "16.11.24",
    "@types/ws": "8.2.2",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "clean-jsdoc-theme": "^3.3.2",
    "eslint": "^8.9.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "jsdoc": "^3.6.10",
    "jsdoc-babel": "^0.5.0",
    "prettier": "^2.5.1",
    "sucrase": "^3.20.3",
    "typescript": "4.5.5",
    "uglifyjs-folder": "^3.0.0"
  },
  "dependencies": {
    "axios": "^0.25.0",
    "discord-api-types": "^0.27.2",
    "tslib": "^2.3.1",
    "ws": "^8.2.3"
  }
}
